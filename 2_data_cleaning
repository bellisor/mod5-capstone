   "cell_type": "code",
   "execution_count": 111,
   "metadata": {},
   "outputs": [],
   "source": [
    "def pegi_age(df):\n",
    "    '''Bin age requirments according to PEGI standards'''\n",
    "    #PEGI Age Ratings 3, 7, 12, 16, 18\n",
    "    \n",
    "    bins = [-1, 0, 3, 7, 12, 16, 2000]\n",
    "    labels = [0, 3, 7, 12, 16, 18]\n",
    "    \n",
    "    df['required_age'] = pd.cut(df['required_age'], bins = bins, labels =labels)\n",
    "    \n",
    "    return df"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 112,
   "metadata": {},
   "outputs": [],
   "source": [
    "age_df = pegi_age(initial_df)\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 113,
   "metadata": {
    "scrolled": true
   },
   "outputs": [
    {
     "data": {
      "text/plain": [
       "0     36584\n",
       "3         4\n",
       "7         8\n",
       "12       48\n",
       "16      207\n",
       "18      758\n",
       "Name: required_age, dtype: int64"
      ]
     },
     "execution_count": 113,
     "metadata": {},
     "output_type": "execute_result"
    }
   ],
   "source": [
    "age_df['required_age'].value_counts().sort_index()"
   ]
  },
