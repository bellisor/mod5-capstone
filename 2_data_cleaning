    "def print_steam_links(df):\n",
    "    url= 'https://store.steampowered.com/app/'\n",
    "    \n",
    "    for i, row in df.iterrows():\n",
    "        appid = row['steam_appid']\n",
    "        name = row['name']\n",
    "        \n",
    "        print(name + ':', url + str(appid))\n",
    "    "
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 126,
   "metadata": {},
