   "source": [
    "# remove overlapping columns\n",
    "steam_clean = merged.drop(['name_steamspy', 'languages', 'steam_appid'], axis=1)\n",
    "# reindex to reorder columns\n",
    "steam_clean = steam_clean[[\n",
    "    'appid',\n",
    "    'name',\n",
    "    'release_date',\n",
    "    'english',\n",
    "    'developer',\n",
    "    'publisher',\n",
