  },
  {
   "cell_type": "code",
   "execution_count": 281,
   "metadata": {},
   "outputs": [],
   "source": [
    "def clean_critic(df):\n",
    "    df = df.copy()\n",
    "    \n",
    "    def parse_critic(x):\n",
    "        if x is not np.nan:\n",
    "            return literal_eval(x)\n",
    "        else:\n",
    "            return{'score': -1, 'url': 'google.com'}\n",
    "    \n",
    "    df['metacritic'] = df['metacritic'].apply(parse_critic)\n",
    "    \n",
    "    #create columns for score and url\n",
    "    \n",
    "    df['meta_score'] = df['metacritic'].apply(lambda x: x['score'])\n",
    "    df['meta_url'] = df['metacritic'].apply(lambda x: x['url'])\n",
    "    \n",
    "    return df"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 282,
   "metadata": {},
   "outputs": [],
   "source": [
    "def clean_rec(df):\n",
    "    df = df.copy()\n",
    "    \n",
    "    def parse_rec(x):\n",
    "        if x is not np.nan:\n",
    "            return literal_eval(x)\n",
    "        else:\n",
    "            return{'total': -1}\n",
    "    \n",
    "    df['recommendations'] = df['recommendations'].apply(parse_rec)\n",
    "    \n",
    "    #create columns for score and url\n",
    "    \n",
    "    df['total_recommendations'] = df['recommendations'].apply(lambda x: x['total'])\n",
    "    \n",
    "    \n",
    "    return df"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 288,
   "metadata": {},
   "outputs": [],
   "source": [
    "mc = clean_critic(df)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 289,
   "metadata": {},
   "outputs": [],
   "source": [
    "mc = clean_rec(mc)"
   ]
  },
