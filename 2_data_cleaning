    "def parse_tags(x):\n",
    "    x = literal_eval(x)\n",
    "    if isinstance(x, dict):\n",
    "        return ';'.join(list(x.keys())[:3])\n",
    "    else:\n",
    "        return np.nan\n",
    "tags.apply(parse_tags).head()"
